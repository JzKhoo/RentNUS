{
  "name": "rentnus",
  "version": "1.0.0",
  "description": "MERN RentNUS App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JzKhoo/RentNUS.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JzKhoo/RentNUS/issues"
  },
  "homepage": "https://github.com/JzKhoo/RentNUS#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.13.5",
    "bcryptjs": "^3.0.3",
    "colors": "^1.4.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.2.1",
    "multer": "^2.0.2",
    "path": "^0.12.7",
    "react-datepicker": "^9.1.0",
    "react-dropzone": "^14.4.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
